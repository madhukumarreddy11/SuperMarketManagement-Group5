<div class="container">
    <table class="table table-bordered table-hover">
        <tr class="bg-success text-white">
            <th>Id</th>
            <th>fName</th>
            <th>LName</th>
            <th>DoB</th>
            <th>Gender</th>
            <th>Mobile</th>
            <th>Email</th>
            <th>UserId</th>
            <th>Password</th>
            <th>ConfirmPassword</th>
            <th>Actions</th>
        </tr>
        @{
            try
            {
                if(TempData["Register"]!=null)
                {
                    foreach(var x in (List<SuperMarketManagement.Models.UserMaster>)TempData["Register"])
                    {
                        <tr>
                        <td>@x.Id</td>
                        <td>@x.fName</td>
                        <td>@x.LName</td>
                        <td>@x.DoB</td>
                        <td>@x.Gender</td>
                        <td>@x.Contactnumber</td>
                        <td>@x.Email</td>
                        <td>@x.UserId</td>
                        <td>@x.Password</td>
                        <td>@x.CPassword</td>
                        <td>
                         <a href="~/User/Approval" class="btn btn-success">Approval</a>

                         <a href="~/User/Reject?id=@x.Id" class="btn btn-danger" style="margin-left:3%">Reject</a>
                        </td>
                        </tr>
                    }
                }
            }
            catch
            {
                
            }
        }
    </table>

</div>
@{
    try
    {
        if(TempData["EditMsg"] !=null)
        {
            <script>
                swal("Good job!", "Request Approved Sucecssfully!", "success");
           </script>
        }
    }
    catch
    {
        
    }
}

@{
    try
    {
        if(TempData["CancelMsg"] !=null)
        {
              <script>
                swal("Good job!","Request Cancelled Sucecssfully!", "success");
           </script>
        }
    }
    catch
    {
        
    }
}

